				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>

	<xs:complexType name="ActionResult">
		<xs:sequence>
			<xs:element name="result" type="xs:string" minOccurs="1"
				default="OK" />
			<xs:element name="Message" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name='listOfStrings'>
		<xs:list itemType="xs:string" />
	</xs:simpleType>


	<xs:complexType name="PartnershipTable">
		<xs:sequence>
			<xs:element name="DomainId" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="PartnershipRecord" type="PartnershipRecord"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PartnershipRecord">
		<xs:sequence>
			<xs:element name="AirlineCode" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="PartnershipAirlineCode" minOccurs="0"
				maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="type" type="PartnershipType" />
							<xs:attribute name="segmentAssociationAllowed" type="xs:boolean"
								default="false" />
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="SegmentAssociationAllowed" type="xs:boolean"
			use="optional" default="false" />
	</xs:complexType>


	<xs:simpleType name="PartnershipType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PARTNER" />
			<xs:enumeration value="SERVICE" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="Pcc">
		<xs:sequence>
			<xs:element name="Ind" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Value" type="xs:string" use="required" />
		<xs:attribute name="DefaultAll" type="xs:boolean" default="false"/>
	</xs:complexType>
	
	<xs:simpleType name="AsOrTnType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="AS" />
			<xs:enumeration value="TN" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="ClientContext">
		<xs:sequence>
                <xs:element name="Name" type="xs:string" />
		</xs:sequence>
	</xs:complexType>


	<xs:element name="RetryRecord">
		<xs:complexType>
			<xs:sequence>

				<xs:element name="RequestType" type="RetryRecordRequestType"
							minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Type of request, for example SPANISH
						</xs:documentation>
					</xs:annotation>
				</xs:element>

				<xs:element name="ServiceLocation" type="RetryRecordServiceLocation"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Location of service (TRIP, S2,..)
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RequestData" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Payload of request that need to be retried.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LastRetryAttempt" type="xs:dateTime"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Timestamp of last retry attempt
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TotalNumberOfRetryAttempts" type="xs:int"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>This field may be also calculated basing on
							values FrequencyOfRetries and EndOfRetry.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CurrentNumberOfRetryAttempt" type="xs:int"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Value of CurrentNumberOfRetryAttempt is
							incremented after each retry execution
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RetryDueTime" type="xs:dateTime"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Timestamp of next retry execution
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FrequencyOfRetries" type="xs:int"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Frequency of retries in minutes
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EndOfRetry" type="xs:dateTime"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Timestamp of scheduled end of retry execution.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Status" type="RetryRecordStatus"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Status of retry execution. Currently supported
							statuses are: NEW, IN_PROGRESS, COMPLETED, FAILED, POST_PROCESSSING_NEEDED
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CreationTimestamp" type="xs:dateTime"
							minOccurs="1">
					<xs:annotation>
						<xs:documentation>Timestamp of requestCreation. Used in file name.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ErrorCause" type="xs:string"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Cause of retry, for example stack trace of exception
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AdditionalData" type="xs:string"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Addidional data needed for retry execution
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="UniqueId" type="xs:string"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Unique Identifier of retry record
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PnrLocator" type="xs:string"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Pnr Locator used for Update
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PnrPartition" type="xs:string"
							minOccurs="0">
					<xs:annotation>
						<xs:documentation>Partition Pnr resides in
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="RetryRecordStatus">
		<xs:annotation>
			<xs:documentation>List of possible Retry record statuses
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="NEW"/>
			<xs:enumeration value="IN_PROGRESS"/>
			<xs:enumeration value="COMPLETED"/>
			<xs:enumeration value="FAILED"/>
			<xs:enumeration value="POST_PROCESSING_NEEDED"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="RetryRecordRequestType">
		<xs:annotation>
			<xs:documentation>List of possible Retry record statuses
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SPANISH"/>
			<xs:enumeration value="CDM"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="RetryRecordServiceLocation">
		<xs:annotation>
			<xs:documentation>List of possible Retry record statuses
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Trip"/>
		</xs:restriction>
	</xs:simpleType>

</xs:schema>
