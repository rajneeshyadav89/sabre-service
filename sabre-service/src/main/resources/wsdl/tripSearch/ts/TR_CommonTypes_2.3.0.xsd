<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://webservices.sabre.com/triprecord"
        xmlns:tns="http://webservices.sabre.com/triprecord" xmlns:pnrb="http://webservices.sabre.com/pnrbuilder/v1_19"
        targetNamespace="http://webservices.sabre.com/triprecord" elementFormDefault="qualified">

        <xs:import namespace="http://webservices.sabre.com/pnrbuilder/v1_19" schemaLocation="../stl/GetReservationSTLRS_1.19.0.xsd"/>

	<xs:element name="TripUpdate">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TripID" type="tns:TripIDType" />
				<xs:element name="Header" type="tns:TripHeaderType"
					minOccurs="0" />
				<xs:element name="POS" type="tns:POSInfoType" />
				<xs:element name="Customer" type="tns:CustomerType"
					minOccurs="0" maxOccurs="unbounded" />
				<xs:element name="Component" type="tns:TripComponentType"
					minOccurs="0" maxOccurs="unbounded" />
				<xs:element name="ModificationTimestamp" type="tns:ModificationTimestampType" />
			</xs:sequence>
			<xs:attributeGroup ref="tns:UpdateAttributes" />
		</xs:complexType>
	</xs:element>
	<xs:complexType name="TripSummaryType">
		<xs:sequence>
			<xs:element name="Trip" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:TripIDType">
							<xs:sequence>
								<xs:element name="Components" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="ComponentID" type="tns:ComponentIDType"
												maxOccurs="unbounded" />
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Content">
				<xs:complexType>
					<xs:choice>
						<xs:any processContents="lax" />
					</xs:choice>
					<xs:anyAttribute namespace="##local"
						processContents="lax" />
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TripHeaderType">
		<xs:sequence>
			<xs:element name="BookingSource" type="tns:BookingSourceType"
				minOccurs="0" />
			<xs:element name="TripName" type="tns:string1to255"
				minOccurs="0" />
			<xs:element name="Memo" type="tns:string1to1024"
				minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
	<xs:complexType name="TripShortInfoType">
		<xs:sequence>
			<xs:element name="TripID" type="tns:TripIDType" />
			<xs:element name="TripName" type="tns:string1to255" />
			<xs:element name="BookingSource" type="tns:BookingSourceType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TripComponentType">
		<xs:complexContent>
			<xs:extension base="tns:TripComponentUpdateType">
				<xs:sequence>
					<xs:element name="Content" type="tns:ComponentContentType"
						minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TripComponentUpdateType">
		<xs:sequence>
			<xs:element name="ComponentID" type="tns:ComponentIDType" />
		</xs:sequence>
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
	<xs:complexType name="POSInfoType">
		<xs:attribute name="Airline" type="tns:string2to3" use="optional" />
		<xs:attribute name="AgencyPCC" type="tns:string3to4" use="required" />
		<xs:attribute name="IATANumber" type="tns:string1to8"
			use="optional" />
		<xs:attribute name="Country" type="tns:string2to3" use="optional" />
		<xs:attribute name="StateProvince" type="tns:string1to8"
			use="optional" />
		<xs:attribute name="ClientCode" type="tns:string1to24"
			use="optional" />
		<xs:attribute name="ClientContextCode" type="tns:string1to5"
			use="optional" />
		<xs:attribute name="DomainId" type="tns:string1to5" use="optional" />
		<xs:attribute name="DomainGroup" type="tns:string1to5"
			use="optional" />
		<xs:attribute name="PrimeHostPartition" type="xs:byte"
			use="optional" />
		<xs:attribute name="AccessId" type="tns:string1to20" use="optional" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
	<xs:complexType name="BookingSourceType">
		<xs:attribute name="Name" type="xs:string" use="optional" />
	</xs:complexType>
	<xs:complexType name="CustomerType">
		<xs:sequence>
			<xs:element name="Name" type="tns:PassengerNameType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Email" type="tns:EmailType" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="Profile" type="tns:ProfileType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Loyalty" type="tns:LoyaltyType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Phone" type="tns:TelephoneType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Address" type="tns:AddressType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Contact" type="tns:ContactType"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attributeGroup ref="tns:UpdateAttributes" />
		<xs:attribute name="ProfileStoredItem" type="xs:boolean"
			use="optional" default="false" />
	</xs:complexType>
	<xs:complexType name="PassengerNameType">
		<xs:attribute name="NamePrefix" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="FirstName" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="LastName" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="MiddleName" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="NameSuffix" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="Language" type="tns:string2" use="optional"
			default="EN" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
	<xs:complexType name="ParsedPhoneNumberType">
		<xs:attribute name="CountryCd" type="tns:string1to5" use="optional" />
		<xs:attribute name="AreaCd" type="tns:string1to20" use="optional" />
		<xs:attribute name="PhoneNumber" type="tns:string1to32"
			use="required" />
		<xs:attribute name="Extension" type="tns:string1to10"
			use="optional" />
	</xs:complexType>
	<xs:complexType name="TelephoneType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="FullPhoneNumber" type="tns:string1to32" />
				<xs:element name="ParsedPhoneNumber" type="tns:ParsedPhoneNumberType" />
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="DeviceType" type="tns:string2" use="required" />
		<xs:attribute name="Vendor" type="tns:string1to60" use="optional" />
		<xs:attribute name="Comment" type="tns:string1to60" use="optional" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
		<xs:attribute name="ProfileStoredItem" type="xs:boolean"
			use="optional" default="false" />
	</xs:complexType>
	<xs:complexType name="PhoneType">
		<xs:attribute name="PhoneNumber" type="xs:string" use="optional" />
		<xs:attribute name="DeviceType" type="tns:string2" use="optional" />
	</xs:complexType>
	<xs:complexType name="EmailType">
		<xs:attribute name="EmailFormatTypeCode" type="tns:string1to4"
			use="optional" />
		<xs:attribute name="EmailAddress" type="tns:string1to120"
			use="optional" />
		<xs:attribute name="PrimaryEmail" type="xs:boolean" use="optional"
			default="false" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
		<xs:attribute name="ProfileStoredItem" type="xs:boolean"
			use="optional" default="false" />
	</xs:complexType>
	<xs:complexType name="LoyaltyType">
		<xs:sequence>
			<xs:element name="Supplier" type="tns:SupplierType"
				minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="MembershipID" type="tns:string1to32"
			use="optional" />
		<xs:attribute name="ProgramType" type="tns:string1to10"
			use="optional" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
		<xs:attribute name="ProfileStoredItem" type="xs:boolean"
			use="optional" default="false" />
	</xs:complexType>
	<xs:complexType name="SupplierType">
		<xs:attribute name="SupplierCode" type="tns:string1to3"
			use="optional" />
		<xs:attribute name="SupplierTypeCode" type="tns:string1to3"
			use="optional" />
		<xs:attribute name="ContextName" type="tns:string1to10"
			use="optional" default="IATA" />
	</xs:complexType>
	<xs:complexType name="ProfileType">
		<xs:attribute name="ProfileID" type="tns:string1to32"
			use="optional" />
		<xs:attribute name="ProfileType" type="tns:string2to3"
			use="optional" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
	<xs:complexType name="ContactType">
		<xs:sequence>
			<xs:element name="Phone" type="tns:TelephoneType"
				minOccurs="0" />
			<xs:element name="Address" type="tns:AddressType"
				minOccurs="0" />
			<xs:element name="Email" type="tns:EmailType" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="FirstName" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="LastName" type="tns:string1to100"
			use="optional" />
		<xs:attribute name="Purpose" type="tns:string1to3" use="optional" />
		<xs:attribute name="MediaType" type="tns:string1to3" use="optional" />
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
	<xs:complexType name="HistoryType">
		<xs:sequence>
			<xs:element name="Event" type="tns:EventType" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventType">
		<xs:sequence>
			<xs:element name="XmlBlob" type="xs:string" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="Date" type="xs:dateTime" use="optional" />
		<xs:attribute name="Version" type="xs:string" use="optional" />
	</xs:complexType>
	<xs:element name="HistoryEvent">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="POS" type="tns:POSInfoType" minOccurs="0" />
				<xs:element ref="tns:TripUpdate" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="Source" type="xs:string" use="optional" />
			<xs:attribute name="Event" type="xs:string" use="optional" />
			<xs:attribute name="Date" type="xs:dateTime" use="optional" />
			<xs:attribute name="Version" type="xs:string" use="optional" />
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ComponentContentType">
		<xs:sequence>
			<xs:element name="XmlBlob" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ModificationTimestampType">
		<xs:attribute name="Timestamp" type="xs:dateTime" use="required" />
	</xs:complexType>
	<xs:complexType name="ComponentIDType">
		<xs:attributeGroup ref="tns:ComponentIDAttributes" />
	</xs:complexType>
	<xs:complexType name="TripIDType">
		<xs:attributeGroup ref="tns:TripIDAttributes" />
	</xs:complexType>
	<xs:attributeGroup name="ComponentIDAttributes">
		<xs:attribute name="Locator" type="tns:string1to32" use="required" />
		<xs:attribute name="Type" type="tns:string1to8" use="required" />
		<xs:attribute name="CreateDate" type="xs:date" use="optional" />
	</xs:attributeGroup>
	<xs:attributeGroup name="TripIDAttributes">
		<xs:attribute name="Locator" type="tns:string6" use="required" />
		<xs:attribute name="CreateDate" type="xs:date" use="optional" />
	</xs:attributeGroup>
	<xs:attributeGroup name="UpdateAttributes">
		<xs:attribute name="SequenceNumber" type="xs:int" use="optional" />
		<xs:attribute name="Action" type="tns:UpdateActions" use="optional"
			default="None" />
	</xs:attributeGroup>
	<xs:simpleType name="UpdateActions">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Add" />
			<xs:enumeration value="Update" />
			<xs:enumeration value="Delete" />
			<xs:enumeration value="None" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GroupActions">
		<xs:restriction base="xs:string">
			<xs:enumeration value="DeleteAll" />
			<xs:enumeration value="ReplaceAll" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SearchCriteriaType">
		<xs:sequence>
			<xs:element name="Criterion" type="tns:SearchCriterionType"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchCriterionType">
		<xs:attribute name="Field" type="tns:GenericFieldType"
			use="required" />
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="MatchingOperator" type="tns:MatchingOperatorType"
			use="optional" default="EQUALS" />
	</xs:complexType>
	<xs:simpleType name="GenericFieldType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FirstName" />
			<xs:enumeration value="LastName" />
			<xs:enumeration value="Email" />
			<xs:enumeration value="ProfileID" />
			<xs:enumeration value="ProfileDomain" />
			<xs:enumeration value="ReservationID" />
			<xs:enumeration value="ReservationDomain" />
			<xs:enumeration value="FlightNumber" />
			<xs:enumeration value="DepartureDate" />
			<xs:enumeration value="ArrivalDate" />
			<xs:enumeration value="DepartureAirport" />
			<xs:enumeration value="ArrivalAirport" />
			<xs:enumeration value="MarketingAirline" />
			<xs:enumeration value="OperatingAirline" />
			<xs:enumeration value="LoyaltyID" />
			<xs:enumeration value="LoyaltyDomain" />
			<xs:enumeration value="PhoneAreaCityCode" />
			<xs:enumeration value="PhoneNumber" />
			<xs:enumeration value="TripName" />
			<xs:enumeration value="ComponentFirstName" />
			<xs:enumeration value="ComponentLastName" />
			<xs:enumeration value="ComponentLoyaltyID" />
			<xs:enumeration value="ComponentLoyaltyDomain" />
			<xs:enumeration value="ComponentPhoneAreaCityCode" />
			<xs:enumeration value="ComponentPhoneNumber" />
			<xs:enumeration value="ComponentEmail" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MatchingOperatorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="EQUALS" />
			<xs:enumeration value="LIKE" />
			<xs:enumeration value="GT" />
			<xs:enumeration value="LT" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PingServicesType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PnrConn" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BaseTripRequest" abstract="true">
		<xs:sequence>
			<xs:element name="ClientContext" type="ClientContext" minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="tns:BaseTripAttributes" />
	</xs:complexType>
	<xs:complexType name="BaseTripResponse" abstract="true">
		<xs:sequence>
			<xs:element name="Success" type="xs:string" minOccurs="0" />
			<xs:element name="Warnings" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Warning" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="ShortText" type="xs:string"
											use="optional" />
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Errors" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Error" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ErrorInfo" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Message" type="xs:string"
													minOccurs="0" />
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="ErrorCode" type="xs:string"
									use="optional" />
								<xs:attribute name="Severity" type="xs:string" use="optional" />
								<xs:attribute name="ErrorMessage" type="xs:string"
									use="optional" />
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="tns:BaseTripAttributes" />
	</xs:complexType>
	<xs:attributeGroup name="BaseTripAttributes">
		<xs:attribute name="ConversationId" type="xs:string" use="optional" />
		<xs:attribute name="Compression" type="xs:boolean" use="optional"
			default="false" />
		<xs:attribute name="EchoToken" use="optional">
			<xs:annotation>
				<xs:documentation>
					A sequence number for additional message
					identification, assigned by
					the requesting host system.
					When a
					request message includes an echo
					token the corresponding response
					message MUST include an echo token
					with an
					identical value.
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:annotation>
					<xs:documentation xml:lang="en">Used for Character
						Strings, length 0 to 64</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:minLength value="0" />
					<xs:maxLength value="64" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="TimeStamp" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>
					Indicates the creation date and time of the
					message in UTC using the
					following format specified by
					ISO 8601;
					YYYY- MM- DDThh:mm:ssZ with
					time values using the 24 hour clock
					(e.g. 20 November 2003, 1:59:38
					pm UTC
					becomes 2003-11-
					20T13:59:38Z).
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Target" type="xs:string" use="optional"
			default="Production">
			<xs:annotation>
				<xs:documentation>Used to indicate whether the request is for
					the
					Test or Production system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Version" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>For all OTA versioned messages, the version of
					the
					message is indicated by a decimal value.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SequenceNmbr" type="xs:nonNegativeInteger"
			use="optional">
			<xs:annotation>
				<xs:documentation>
					Used to identify the sequence number of the
					transaction as assigned by
					the sending system; allows
					for an
					application to process messages
					in
					a certain order or to request a
					resynchronization of messages in the
					event
					that a system has been
					off-line and needs to retrieve
					messages
					that were missed.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PrimaryLangID" type="xs:language"
			use="optional">
			<xs:annotation>
				<xs:documentation>
					Identifies the primary language preference for the
					form of travel represented
					in a collection. The
					human language is
					identified by ISO 639
					codes.
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AltLangID" type="xs:language" use="optional">
			<xs:annotation>
				<xs:documentation>
					Identifies the primary language preference for the
					form of travel represented
					in a collection. The
					human language is
					identified by ISO 639
					codes.
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReturnContent" type="xs:boolean" use="optional"
			default="false" />
	</xs:attributeGroup>
	<xs:simpleType name="OTA_CodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for codes in the
				OTA
				code tables.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Z]{1,3}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="UniqueID_Type">
		<xs:annotation>
			<xs:documentation xml:lang="en">An identifier used to
				uniquely reference an object in the system
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="tns:UniqueID_Group" />
	</xs:complexType>
	<xs:attributeGroup name="UniqueID_Group">
		<xs:attribute name="ID" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					A unique identifying
					value assigned by the creating
					system. The ID
					attribute may be used
					to reference a
					primary-key value within a database or in a
					particular implementation.
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1" />
					<xs:maxLength value="32" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Type" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1" />
					<xs:maxLength value="32" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CustomerLoyaltyGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Program rewarding
				frequent use by accumulating credits for services
				provided by
				vendors.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ProgramID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifier to
					indicate
					the company owner of the loyalty program.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MembershipID" type="xs:string" use="optional">
			<xs:annotation>
