	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Provides address
				information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AddressLine" type="tns:string1to60"
				minOccurs="0" maxOccurs="4">
				<xs:annotation>
					<xs:documentation xml:lang="en">When the address is
						unformatted (FormattedInd="false") these lines will
						contain free
						form address details. When the address is formatted and street
						number and street name must be sent
						independently, the street
						number will be sent using StreetNmbr, and the street name will be
						sent in the first
						AddressLine occurrence.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CityName" type="tns:string1to60"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">City (e.g.,
						Dublin),
						town, or postal station (i.e., a postal service
						territory,
						often
						used in a military address).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PostalCd" type="tns:string1to16"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Post Office Code
						number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StateCode" type="tns:string1to8"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">State or Province
						Code
						based on Postal Information(e.g., TX for Texas).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryCode" type="tns:string2"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Country name as
						a Code
						(e.g., US or USA for United States of America).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="LocationTypeCode" type="tns:string1to3"
			use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines the
					Location of
					the address (e.g. home, business, other). Refer to
					Code
					List
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AddressUsageTypeCode" type="tns:string1to3"
			use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Describes the
					use of the
					address (e.g. mailing, delivery, billing, etc.).
					Refer
					to
					OTA Code
					List Address Use Type (AUT).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Attention" type="tns:string1to40"
			use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Address Attention
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="tns:UpdateAttributes" />
	</xs:complexType>
